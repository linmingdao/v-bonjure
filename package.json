{
  "name": "v-base-core",
  "version": "1.0.0",
  "description": "v-base-core, a generic framework for font-end development by vue",
  "private": true,
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1"
  },
  "scripts": {
    "doc": "node ./doc/app.js",
    "init": "node ./build/bin/init.js",
    "component": "node ./build/bin/component.js",
    "serve": "cross-env BUILD_MODE=devServer node ./build/bin/serve.js",
    "build": "cross-env BUILD_MODE=appBundle node ./build/bin/build.js",
    "deploy": "cross-env BUILD_MODE=appBundle node ./build/bin/deploy.js",
    "format": "cross-env MODE=app node ./build/bin/format.js"
  },
  "keywords": [
    "vue",
    "webpack",
    "v-base-core",
    "JavaScript"
  ],
